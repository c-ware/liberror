/* Verify that all the macros compile (from liberror's m4 implementation) */

#include <stdio.h>

#if !defined(__QuasiBSD__)
#include <stdlib.h>
#endif

include(src/liberror.m4)

int main() {
    char *x = "foo";
    int y = 9;
    char *z = CWUTILS_NULL;

    LIBERROR_IS_NULL_M4(x, "foo");
    LIBERROR_IS_OOB_M4(3, 10);
    LIBERROR_IS_OOB_M4(y, 10);
    LIBERROR_IS_NEGATIVE_M4(y, "y");
    LIBERROR_IS_POSITIVE_M4(y, "y");
    LIBERROR_IS_VALUE_M4(y, 5, "y", "5");
    LIBERROR_MALLOC_FAILURE_M4(z, "z");
    LIBERROR_FILE_OPEN_FAILURE_M4(z, "z", "test.txt");
    

    return 0;
}
